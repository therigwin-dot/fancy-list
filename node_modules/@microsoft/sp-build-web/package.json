{
  "name": "@microsoft/sp-build-web",
  "version": "1.21.1",
  "description": "SharePoint Framework build rig for web projects",
  "license": "https://aka.ms/spfx/license",
  "homepage": "http://aka.ms/spfx",
  "engines": {
    "node": ">=18.17.1 <19.0.0 || >=22.14.0 < 23.0.0"
  },
  "main": "lib-commonjs/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib-dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./lib-dts/index.d.ts"
    },
    "./lib/*": {
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js",
      "types": "./lib-dts/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "dependencies": {
    "@microsoft/gulp-core-build": "3.19.0",
    "@microsoft/gulp-core-build-sass": "4.17.3",
    "@microsoft/gulp-core-build-serve": "3.13.0",
    "@microsoft/gulp-core-build-typescript": "8.6.3",
    "@microsoft/gulp-core-build-webpack": "6.0.3",
    "gulp": "4.0.2",
    "semver": "~7.3.2",
    "webpack": "~5.95.0",
    "yargs": "~4.6.0",
    "@microsoft/sp-build-core-tasks": "1.21.1"
  },
  "devDependencies": {
    "@rushstack/heft": "0.71.0",
    "@types/semver": "7.3.8",
    "eslint": "8.57.1",
    "@msinternal/internal-node-rig": "0.1.0"
  },
  "scripts": {
    "build": "heft test --clean",
    "clean": "heft clean",
    "start": "heft build-watch",
    "_phase:depscan": "heft depscan",
    "_phase:tool-build": "heft run --only build -- --clean",
    "_phase:tool-build:incremental": "heft run --only build --"
  }
}