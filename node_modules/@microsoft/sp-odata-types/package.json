{
  "name": "@microsoft/sp-odata-types",
  "version": "1.21.1",
  "description": "TypeScript interfaces for working with SharePoint REST APIs",
  "license": "https://aka.ms/spfx/license",
  "homepage": "http://aka.ms/spfx",
  "main": "lib-commonjs/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib-dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./lib-dts/index.d.ts"
    },
    "./lib/*": {
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js",
      "types": "./lib-dts/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
  },
  "devDependencies": {
    "@rushstack/heft": "0.71.0",
    "eslint": "8.57.1",
    "@msinternal/internal-node-rig": "0.1.0"
  },
  "dependencies": {
    "@swc/helpers": "0.5.12",
    "tslib": "2.3.1"
  },
  "engines": {
    "node": ">=18.17.1 <19.0.0 || >=22.14.0 < 23.0.0"
  },
  "scripts": {
    "build": "heft test --clean",
    "clean": "heft clean",
    "deploy": "heft dev-deploy",
    "start": "heft build-watch --serve",
    "_phase:tool-build": "heft run --only build -- --clean",
    "_phase:tool-build:incremental": "heft run --only build --",
    "_phase:depscan": "heft depscan"
  }
}