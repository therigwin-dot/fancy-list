{
  "name": "@microsoft/sp-lodash-subset",
  "version": "1.21.1",
  "description": "Essential lodash functions for SharePoint",
  "license": "https://aka.ms/spfx/license",
  "homepage": "http://aka.ms/spfx",
  "main": "lib-commonjs/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib-dts/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./lib-dts/index.d.ts"
    },
    "./lib/*": {
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js",
      "types": "./lib-dts/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "devDependencies": {
    "@rushstack/heft": "0.71.0",
    "@rushstack/node-core-library": "5.12.0",
    "eslint": "8.57.1",
    "lodash": "4.17.21",
    "lodash-cli": "~4.17.5",
    "@ms/spfx-internal-heft-plugins": "1.5.2",
    "@microsoft/spfx-heft-plugins": "1.21.1",
    "@microsoft/sp-module-interfaces": "1.21.1",
    "@msinternal/internal-non-tool-node-rig": "0.1.0",
    "@ms/size-auditor-interfaces": "0.1.0",
    "@msinternal/jest-transformers": "0.1.0"
  },
  "dependencies": {
    "@swc/helpers": "0.5.12",
    "@types/lodash": "4.14.117",
    "tslib": "2.3.1"
  },
  "engines": {
    "node": ">=18.17.1 <19.0.0 || >=22.14.0 < 23.0.0"
  },
  "scripts": {
    "build": "heft test --clean",
    "clean": "heft clean",
    "test": "heft test",
    "test-watch": "heft test-watch",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:build:incremental": "heft run --only build --",
    "_phase:depscan": "heft depscan",
    "_phase:test": "heft run --only test -- --clean",
    "_phase:test:incremental": "heft run --only test --"
  }
}